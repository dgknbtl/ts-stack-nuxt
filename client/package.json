      "prettier --write"
    ]
  },
  "dependencies": {
    "@nuxt/eslint": "^0.7.4",
    "nuxt": "^3.15.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite-plugin-eslint2": "^5.0.3"
  }
}
